{
  "_version": "1.1.0",
  "sap.app": {
    "id": "anotherone",
    "type": "application",
    "title": "Manage Employees",
    "description": "Sample Employee Management Application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "dataSources": {
      "AdminService": {
        "uri": "/odata/v4/admin/",
        "type": "OData",
        "settings": {
          "odataVersion": "4.0"
        }
      }
    },
    "-sourceTemplate": {
      "id": "ui5template.basicSAPUI5ApplicationProject",
      "-id": "ui5template.smartTemplate",
      "-version": "1.40.12"
    }
  },
  "sap.cloud": {
    "public": true,
    "service": "demo"
  },
  "sap.ui5": {
    "dependencies": {
      "libs": {
        "sap.fe.templates": {}
      }
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "uri": "i18n/i18n.properties"
      },
      "": {
        "dataSource": "AdminService",
        "settings": {
          "synchronizationMode": "None",
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true,
          "groupProperties": {
            "default": {
              "submit": "Auto"
            }
          }
        }
      }
    },
    "routing": {
      "routes": [
        {
          "pattern": ":?query:",
          "name": "EmployeesList",
          "target":"EmployeesList"
          
        },
        {
          "pattern": "Employees({key}):?query:",
          "name": "EmployeesDetails",
          "target": "EmployeesDetails"
        },
        {
          "pattern": "Employees({key})/Roles({key2}):?query:",
          "name": "RolesDetails",
          "target": "RolesDetails"
          
        },
        {
          "pattern": "Employees({key})/Projects({key2}):?query:",
          "name": "ProjectsDetails",
          "target": "ProjectsDetails"

        },
        {
          "pattern": "Employees({key})/Addresses({key2}):?query:",
          "name": "AddressDetails",
          "target": "AddressDetails"

        }
      ],
      "targets": {
        "EmployeesList": {
          "type": "Component",
          "id": "EmployeesList",
          "name": "sap.fe.templates.ListReport",
          "options": {
            "settings": {
              "entitySet": "Employees",
              "initalLoad": true,
              "navigation": {
                "Employees": {
                  "detail": {
                    "route": "EmployeesDetails"
                  }
                }
              }
            }
          }
        },
        "EmployeesDetails": {
          "type": "Component",
          "id": "EmployeesDetails",
          "name": "sap.fe.templates.ObjectPage",
          "options": {
            "settings": {
              "entitySet": "Employees",
              "navigation": {
                "Roles": {
                  "detail": {
                    "route": "RolesDetails"
                  }
                },
                "Projects": {
                  "detail": {
                    "route": "ProjectsDetails"
                  }
                },
                "Addresses": {
                  "detail": {
                    "route": "AddressDetails"
                  }
                }
              }
            }
          }
        },
        "RolesDetails": {
          "type": "Component",
          "id": "RolesDetails",
          "name": "sap.fe.templates.ObjectPage",
          "options": {
            "settings": {
              "entitySet": "Roles"
            }
          }
        },
        "ProjectsDetails": {
          "type": "Component",
          "id": "ProjectsDetails",
          "name": "sap.fe.templates.ObjectPage",
          "options": {
            "settings": {
              "entitySet": "Projects"
            }
          }
        },
        "AddressDetails": {
          "type": "Component",
          "id": "AddressDetails",
          "name": "sap.fe.templates.ObjectPage",
          "options": {
            "settings": {
              "entitySet": "Addresses"
            }
          }
        }
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "fullWidth": false
  },
  "sap.fiori": {
    "registrationIds": [],
    "archeType": "transactional"
  }
}
