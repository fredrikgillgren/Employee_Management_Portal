{
  "name": "Employee_Management_Portal",
  "version": "1.0.0",
  "files": [
    "db/",
    "srv/",
    "app/",
    "index.cds",
    "index.js"
  ],
  "dependencies": {
    "@sap/cds": "^8.6.0",
    "@sap/cds-dk": "^8.6.1",
    "@sap/cds/common": "^5.0.0",
    "@sap/cds-fiori": "^5.0.0",
    "express": "^4"
  },
  "devDependencies": {
    "@cap-js/cds-types": "^0.7.0",
    "@sap/cds-dk": "^5.0.0",
    "@cap-js/sqlite": "^1",
    "@sapui5/types": "^1.131.1",
    "@types/openui5": "^1.131.0",
    "cds-plugin-ui5": "^0.9.3"
  },
  "cds": {
    "requires": {
      "db": {
        "kind": "sqlite",
        "model": "db/schema"
      }
    }
  },
  "scripts": {
    "start": "cds-serve",
    "watch-project1": "cds watch --open project1/index.html?sap-ui-xx-viewCache=false --livereload false",
    "watch-project2": "cds watch --open project2/webapp/index.html?sap-ui-xx-viewCache=false",
    "watch-employeeroleprojects": "cds watch --open employeeroleprojects/webapp/index.html?sap-ui-xx-viewCache=false",
    "watch-test": "cds watch --open test/webapp/index.html?sap-ui-xx-viewCache=false",
    "watch-emp": "cds watch --open emp/webapp/index.html?sap-ui-xx-viewCache=false",
    "watch-new": "cds watch --open new/webapp/index.html?sap-ui-xx-viewCache=false",
    "watch-emportal": "cds watch --open emportal/webapp/index.html?sap-ui-xx-viewCache=false",
    "watch-anotherone": "cds watch --open anotherone/webapp/index.html?sap-ui-xx-viewCache=false"
  },
  "workspaces": [
    "app/*"
  ],
  "sapux": [
    "app/project1",
    "app/project2",
    "app/employeeroleprojects",
    "app/test",
    "app/emp",
    "app/new",
    "app/emportal",
    "app/emportal",
    "app/anotherone"
  ]
}
